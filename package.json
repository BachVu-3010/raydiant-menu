{
  "private": "true",
  "name": "raydiant-menu",
  "version": "1.1.4",
  "description": "React Menu Component for Raydiant Menu apps",
  "main": "index.js",
  "repository": "https://github.com/mirainc/raydiant-menu",
  "author": "mirainc",
  "license": "MIT",
  "scripts": {
    "start": "tsc && styleguidist server --config styleguide.config.js",
    "tsc": "tsc",
    "build": "tsc && copyfiles -u 2 src/lib/**/*.css build/lib && node tasks/copy-package-json.js",
    "build:watch": "tsc -w",
    "build:link": "yarn build && cd build/lib && yarn unlink && yarn link",
    "lint": "tslint --project .",
    "test": "jest src --env=jsdom",
    "deploy": "sh tasks/npm-publish.sh",
    "styleguide:build": "tsc && styleguidist build",
    "encryptenv": "jet encrypt docker/deploy.env docker/deploy.env.encrypted --key-path=mirainc_raydiant-menu.aes",
    "decryptenv": "jet decrypt docker/deploy.env.encrypted docker/deploy.env --key-path=mirainc_raydiant-menu.aes"
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "lodash.isequal": "^4.5.0",
    "lodash.memoize": "^4.1.2",
    "moment": "^2.29.1",
    "qrcode": "^1.4.4",
    "raydiant-kit": "^4.6.0",
    "react": "16.8.0",
    "react-dom": "16.8.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.2",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.memoize": "^4.1.6",
    "@types/qrcode": "^1.4.0",
    "@types/react": "16.8.0",
    "@types/react-dom": "16.8.0",
    "@types/should-sinon": "^0.0.8",
    "@types/sinon": "^10.0.2",
    "@types/styled-components": "^5.1.10",
    "@types/webfontloader": "^1.6.32",
    "copyfiles": "^2.0.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "fetch-mock": "^9.11.0",
    "fs-extra": "^7.0.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-enzyme": "^6.0.4",
    "lint-staged": "^7.2.0",
    "node-fetch": "^2.6.1",
    "prettier": "^1.14.2",
    "react-docgen-typescript": "^1.9.1",
    "react-styleguidist": "^7.3.8",
    "should": "^13.2.3",
    "should-sinon": "^0.0.6",
    "sinon": "^11.1.1",
    "style-loader": "^0.22.1",
    "ts-jest": "^23.1.4",
    "ts-loader": "^4.4.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "rootDir": "src",
    "setupFiles": [
      "<rootDir>/setupTests.ts"
    ]
  }
}
